<% include ./partials/header %>
<div class = "ui container">
	<h1>All My Shoes </h1>


	<% shoesPassedIn.forEach(function(shoes){ %>
		<label>Shoe </label>
		<p>type: <%= shoes.type %></p>
		<p>color: <%= shoes.color %></p>
		<p>size: <%= shoes.size %></p>
		<p>preferred: <%= shoes.preferred %></p>
	<%});%>
	

</div>
